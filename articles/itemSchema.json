{   
    "$schema": "http://json-schema.org/schema",
    "$id": "https://json-schema.org/draft/2020-12/schema",
    "title": "Articles",
    "description": "Describes an ArticlesItem object",
    "type": "object",
    "properties":
        {
           "name": {
                "type": "string"
            },
           "date": {
                "type": "string"
            },
           "link": {
                "type": "string"
            },
            "labels": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
           "content": {
                "type": "string"
            }
        },
    "required": ["name", "date", "link", "labels", "content"],
    "additionalProperties": false
   }
